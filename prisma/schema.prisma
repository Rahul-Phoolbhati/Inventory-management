generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Product {
  id                 String  @id @default(cuid())
  name               String  @unique
  description        String
  stock_quantity     Int      @default(0)
  low_stock_threshold Int     @default(0)
  created_at         DateTime @default(now())
  updated_at         DateTime @updatedAt
}
